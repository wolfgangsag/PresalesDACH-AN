                            </bmobile:col>
                            <bmobile:flabel styleclasses="col-lg-2" name="Status">
                            </bmobile:flabel>
                            <bmobile:col styleclasses="col-lg-4">
                                <bmobile:ffield placeholder="Status" valueprop="costatus" maxlength="20" formautocomplete="false" required="true" validationmessage="Required">
